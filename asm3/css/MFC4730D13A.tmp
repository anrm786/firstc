@charset "utf-8";
/* CSS Document */
body{
	font-size: 16px;
	color:#fff;
	background-color: lightcoral;
	text-align: center;
	
}
h3{
	color: green;
	background-color: azure;
	margin-left:10%;
	margin-right:10%;
	margin-top: 10px;
	border: 2px solid black;
	font-size: 25px;
}
#ls{

	margin-top: 30px;
}
.cl img{
	width:140px;
	height: 140px;
	
}
.btl,.btg{
	margin-top: 30px;
	position: absolute;
	
}
.col-3 img{
	margin-top: 20px;
	border: 2px solid red;

	
}
.pk{
	width: 100%;
	height:65vw;
	border: 3px solid black;
	
}

@media (max-width:768px){
	
	.cl img{
	width:100px;
	height: 100px;
	
}
	.pk{
		height:65vw;
	}
	h3{
		font-size: 16px;
	}
}
@media (max-width:575px){
	
	.cl img{
	width:80px;
	height: 80px;
	
}
	.pk{
		height:65vw;
	}
 
}
@media (max-width:432px){
	
		.cl img{
	width:60px;
	height: 60px;
	
}
	.pk{
		height:65vw;
	}
}
	@media (max-width:354px){
	
		.cl img{
	width:40px;
	height: 40px;
	
}
		.pk{
		height:65vw;
	}
    
}

