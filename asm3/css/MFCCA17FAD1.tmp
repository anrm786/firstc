@charset "utf-8";
/* CSS Document */
body{
	font-size: 16px;
	color:#fff;
	background-color: lightcoral;
	text-align: center;
	
}
#ls{
	margin-top: 30px;
}
.cl img{
	width:140px;
	height: 140px;
	
}
.col-3 img{
	margin-top: 20px;
	border: 2px solid red;
	
}
.pk{
	width: 80%;
	border: 3px solid black;
}

@media (max-width:768px){
	
	.cl img{
	width:100px;
	height: 100px;
	
}
}
@media (max-width:575px){
	
	.cl img{
	width:80px;
	height: 80px;
	
}
 
}
@media (max-width:432px){
	
		.cl img{
	width:60px;
	height: 60px;
	
}
	@media (max-width:354px){
	
		.cl img{
	width:40px;
	height: 40px;
	
}
    
}

