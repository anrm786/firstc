@charset "utf-8";
/* CSS Document */

.jumbotron{
	height: 205px;
	background-color: #0B5888;
	border-bottom: 2px solid #fff;
	width: 100%;
}
.topblock{
	width: 100%;
	background-color: #003869;
	height: 28px;
	top: 0;
	position: absolute;
	border-bottom: 2px solid #0479B5;
}
.textab{
	margin-left:14%;
	margin-right:14%;
	
}
.linktop li a{
	font-weight: 400;
	font-size: 10px;
	color:#DAE2BD;
}
.kinktop ul{
	list-style: none;
}
.linktop li{
	
	display: inline;
}
.imgl{
	position: absolute;
	top:36px;
	left:36px;
}
.nav-item{
	height: 20px;
	width: auto;
	background-color: red;
}