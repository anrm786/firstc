@charset "utf-8";
/* CSS Document */

.jumbotron{
	height: 205px;
	background-color: #0B5888;
	border-bottom: 2px solid #fff;
	width: 100%;
}
.topblock{
	width: 100%;
	background-color: #003869;
	height: 28px;
	top: 0;
	position: absolute;
	border-bottom: 2px solid #0479B5;
}
.textab{
	margin-left:14%;
	margin-right:14%;
	
}
.linktop li a{
	font-weight: 400;
	font-size: 10px;
	color:#DAE2BD;
}
.kinktop ul{
	list-style: none;
}
.linktop li{
	
	display: inline;
}
.imgl{
	position: absolute;
	top:36px;
	left:36px;
}
.nav-item{
	width: auto;
	margin-left: 12px;
	font-weight: bold;
	position:relative;
	color: white;
}
.menuback{
	background-color: rgb(0, 56, 105);
	margin-bottom: 40px;
	height: 40px;
	margin-top: 70px;
	
}
.btn-d{
	position: absolute;
	right:20px;
	float: right;
	margin-right: 20px;
	font-weight: bold;
	display: inline-block;
}
.vl{
	border-left: 1px solid red;
height: 40px;
	margin-top:-10px;
	
}